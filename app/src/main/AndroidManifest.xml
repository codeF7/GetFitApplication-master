<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gabe.getfitapplication">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GetFitApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Display"></activity>
        <activity android:name=".Session"></activity>
        <activity android:name=".Payment"></activity>
        <activity android:name=".Signature"></activity>
        <activity android:name=".Receipt"></activity>
        <activity android:name=".Success"></activity>
        <activity android:name=".AddCustomer"></activity>



    </application>

</manifest>